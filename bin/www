#!/usr/bin/env node
require('events').EventEmitter.defaultMaxListeners = Infinity;
const logger = require('logger').logger;

const webRunner = require('./runners/web');

try {
  webRunner();

  logger.info(
    '\n\nReady to work\n\n'
  );
} catch (err) {
  //logger.error(err);
  console.log(err);
	process.exit(1);
}
